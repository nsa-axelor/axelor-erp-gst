<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

    <grid model="com.axelor.apps.gst.db.State" title="State Grid" name="state-grid">
        <field name="name" />
        <field name="country" />
    </grid>

    <form model="com.axelor.apps.gst.db.State" title="State Form" name="state-form">
        <panel>
            <field name="name" />
            <field name="country" />
        </panel>
    </form>

</object-views>
