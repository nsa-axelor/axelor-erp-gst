<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

    <grid model="com.axelor.apps.base.db.Company" title="Company Grid" name="company-grid" id="gst-company-grid" extension="true">
        <field name="name" />
        <field name="logo" widget="image" />
        <field name="contact" />
        <field name="gstin" />
        <field name="bankDetilList" />
        <field name="address" />
    </grid>

    <form model="com.axelor.apps.base.db.Company" title="Company Form" name="company-form" id="gst-company-form" extension="true">
    	<extend target="panel[@name='mainPanel']">
    		<insert position="after">
    			<panel colSpan="9">
                <field name="gstin" colSpan="12" max="15" />
            </panel>
    		</insert>
    	</extend>
    </form>

</object-views>
