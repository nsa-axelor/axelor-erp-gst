<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.2.xsd">

	<grid model="com.axelor.apps.base.db.ProductCategory" title="Product Category Grid" name="product-category-grid" id="gst-product-categotry-grid" extension="true">
		<field name="gstRate" />
	</grid>

	<form model="com.axelor.apps.base.db.ProductCategory" title="Product Category Form" name="product-category-form" id="gst-product-categotry-form" extension="true">

		<extend target="panel[@name='mainPanel']">
			<insert position="after">
				<panel>
					<field name="gstRate" />
				</panel>
			</insert>
		</extend>

	</form>

</object-views>
